{
  "name": "borrower",
  "version": "0.3.0",
  "description": "Forget complexity of Require.js config, don't worry about running Bower install.",
  "main": "src/borrower.js",
  "scripts": {
    "test": "jshint lib/ bin/ && mocha --harmony",
    "postinstall": "bin/cli.js init"
  },
  "author": "Jonathan Sanchez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/j-san/borrower.git"
  },
  "bin": {
    "borrower": "bin/cli.js"
  },
  "dependencies": {
    "bower-requirejs": "^1.1.2",
    "chaining": "^0.3.2",
    "colors": "^1.0.3",
    "commander": "^2.5.0",
    "lodash": "^2.4.1",
    "q": "^1.1.1",
    "requirejs": "^2.1.15",
    "thunkify": "^2.1.2"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "co-mocha": "^1.1.0",
    "jshint": "^2.5.10",
    "mz": "^1.2.1",
    "rimraf": "^2.2.8"
  },
  "peerDependencies": {
    "bower": "^1.x"
  },
  "jshintConfig": {
    "curly": true,
    "debug": true,
    "eqnull": true,
    "eqeqeq": true,
    "undef": true,
    "unused": true,
    "node": true,
    "devel": true,
    "esnext": true,
    "noyield": true,
    "maxcomplexity": 5,
    "globals": {
      "describe": true,
      "it": true,
      "before": true,
      "after": true,
      "beforeEach": true,
      "afterEach": true
    }
  }
}